{
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php@0.7.1"
    }
  ],
  "routes": [
    { "src": "/(.*?\\.(jpg|jpeg|png|gif|svg|ico|css|js))", "dest": "/$1" },
    { "src": "/admin", "dest": "/admin.php" },
    { "src": "/go/(?<id>.*)", "dest": "/go.php?id=$id" },
    { "src": "/fix", "dest": "/fix.php" },
    { "src": "/(.*)", "dest": "/index.php" }
  ]
}
